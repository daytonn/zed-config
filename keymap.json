[
  {
    "bindings": {
      "alt-,": "zed::OpenSettings",
      "ctrl-,": ["workspace::SendKeystrokes", "ctrl-e ,"],
      "alt-super-,": "zed::OpenKeymap",
      "ctrl-alt-\\": "workspace::ToggleRightDock",
      "alt-shift-enter": "workspace::ToggleZoom",
      "alt-{": "pane::ActivatePrevItem",
      "alt-}": "pane::ActivateNextItem",
      "ctrl-`": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-f": "editor::MoveRight",
      "ctrl-shift-f": "editor::SelectRight",
      "ctrl-b": "editor::MoveLeft",
      "super-b": "editor::MoveToPreviousWordStart",
      "ctrl-shift-b": "editor::SelectLeft",
      "super-shift-b": "editor::SelectToPreviousWordStart",
      "ctrl-n": "editor::MoveDown",
      "super-f": "editor::MoveToNextWordEnd",
      "ctrl-shift-n": "editor::SelectDown",
      "ctrl-p": "editor::MoveUp",
      "ctrl-shift-p": "editor::SelectUp",
      "ctrl-e": "editor::MoveToEndOfLine",
      "ctrl-shift-e": "editor::SelectToEndOfLine",
      "ctrl-a": "editor::MoveToBeginningOfLine",
      "ctrl-shift-a": "editor::SelectToBeginningOfLine",
      "ctrl-`": "workspace::ToggleBottomDock",
      "ctrl-j": "editor::JoinLines",

      "super-p": "editor::MoveToBeginning",
      "super-n": "editor::MoveToEnd",

      "ctrl-alt-l": "editor::ConvertToLowerCase",
      "ctrl-shift-alt-u": "editor::ConvertToUpperCase",
      "ctrl-alt-u": "editor::ConvertToTitleCase",
      "alt-s": "editor::SortLinesCaseSensitive",

      "ctrl-k": "editor::DeleteToEndOfLine",
      "ctrl-shift-k": "editor::DeleteLine",

      "super-d": "pane::SplitRight",
      "super-h": "pane::SplitDown",
      "alt-super-2": "pane::SplitRight",

      "ctrl-alt-p": "editor::MoveLineUp",
      "ctrl-alt-n": "editor::MoveLineDown",

      "ctrl-shift-alt-p": "editor::AddSelectionAbove",
      "ctrl-shift-alt-n": "editor::AddSelectionBelow",
      "ctrl-d": "editor::Delete",

      "ctrl-shift-alt-super-p": "editor::CopyPath",

      "ctrl-left": "editor::Fold",
      "ctrl-right": "editor::UnfoldLines",

      "alt-a": "editor::SelectAll",

      "alt-shift-d": "editor::DuplicateLineDown",

      "ctrl-\\": ["workspace::ActivatePaneInDirection", "Right"],
      "ctrl-|": ["workspace::ActivatePaneInDirection", "Left"],
      "alt-d": ["editor::SelectNext", { "replace_newest": false }],
      "alt-super-d": "editor::UndoSelection",
      "ctrl-enter": "editor::NewlineBelow",
      "ctrl-shift-enter": "editor::NewlineAbove",
      "ctrl-,": ["workspace::SendKeystrokes", "ctrl-e ,"],
      "ctrl-<": ["workspace::SendKeystrokes", "ctrl-e , ctrl-enter"],
      "alt-super-=": ["workspace::SendKeystrokes", "=>"],
      "alt-super-.": ["workspace::SendKeystrokes", "|>"],
      "ctrl-alt-super-=": ["workspace::SendKeystrokes", "() space => space {} left left left left left left left"],
      "ctrl-;": ["workspace::SendKeystrokes", "ctrl-e ;"],
      "ctrl-:": ["workspace::SendKeystrokes", "ctrl-e ; ctrl-enter"],
      "alt-f": ["buffer_search::Deploy", { "replace_enabled": true }],
      "alt-shift-f": "pane::DeploySearch"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-n": null,
      "ctrl-p": null,
      "alt-super-n": "workspace::NewFile",
      "ctrl-t": "file_finder::Toggle",
      "alt-t": "file_finder::Toggle",
      "ctrl-alt-super-p": "command_palette::Toggle",
      "ctrl-alt-super-f": "outline::Toggle"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "ctrl-n": "menu::SelectNext",
      "ctrl-p": "menu::SelectPrev",
      "alt-super-n": "workspace::NewFile"
    }
  },
  {
    "context": "BufferSearchBar && !in_replace > Editor",
    "bindings": {
      "ctrl-p": "search::PreviousHistoryQuery",
      "ctrl-n": "search::NextHistoryQuery"
    }
  },
  {
    "context": "Picker || menu",
    "bindings": {
      "ctrl-p": "menu::SelectPrev",
      "ctrl-n": "menu::SelectNext"
    }
  },
  {
    "context": "ProjectSearchBar > Editor",
    "bindings": {
      "ctrl-p": "search::PreviousHistoryQuery",
      "ctrl-n": "search::NextHistoryQuery"
    }
  },
  {
    "context": "Editor && (showing_code_actions || showing_completions)",
    "bindings": {
      "ctrp-p": "editor::ContextMenuPrev",
      "ctrl-n": "editor::ContextMenuNext"
    }
  },
  {
    "context": "Picker || menu",
    "bindings": {
      "ctrl-p": "menu::SelectPrev",
      "ctrl-n": "menu::SelectNext"
    }
  },
  {
    "context": "Editor && (showing_code_actions || showing_completions)",
    "bindings": {
      "up": "editor::ContextMenuPrev",
      "ctrl-p": "editor::ContextMenuPrev",
      "down": "editor::ContextMenuNext",
      "ctrl-n": "editor::ContextMenuNext",
      "pageup": "editor::ContextMenuFirst",
      "pagedown": "editor::ContextMenuLast"
    }
  }
]
